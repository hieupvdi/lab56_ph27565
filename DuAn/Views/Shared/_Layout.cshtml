                <div class="sb-sidenav-menu">
                    <div class="nav">

                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">
                            <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                            Trang Chủ
                        </a>
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="DanhSachsp">
                             <div class="sb-nav-link-icon"><i class="fas fa-table"></i></div>
                            Danh Sách Sản Phẩm
                        </a>
                      <a class="nav-link" asp-area="" asp-controller="Product" asp-action="ShowOld">
                             <div class="sb-nav-link-icon"><i class="fas fa-table"></i></div>
                            ShowOld
                        </a>
 
                          @*table bảng con*@
                        <a class="nav-link collapsed" href="#" data-bs-toggle="collapse" data-bs-target="#collapseLayouts" aria-expanded="false" aria-controls="collapseLayouts">
                            <div class="sb-nav-link-icon"><i class="fas fa-columns"></i></div>
                            QL Sản Phẩm
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>
                        <div class="collapse" id="collapseLayouts" aria-labelledby="headingOne" data-bs-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" asp-area="" asp-controller="Product" asp-action="ShowAllProduct">Sản Phẩm</a>
                                <a class="nav-link" asp-area="" asp-controller="Color" asp-action="ShowAllColor">Màu</a>
                                <a class="nav-link" asp-area="" asp-controller="Size" asp-action="ShowAllSize">Size</a>
                                <a class="nav-link" asp-area="" asp-controller="Category" asp-action="ShowAllCategory">Loại Giầy</a>

                            </nav>
                        </div>

                        <a class="nav-link" >
                            <div class="sb-nav-link-icon"><i class="fas fa-chart-area"></i></div>
                            Thông Kê
                        </a>
                           <a class="nav-link" sp-area="" asp-controller="Home" asp-action="ViewBag_ViewData">
